{
  "name": "src",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "start": "vite",
    "server": "tsx --watch ./src/server/server.ts",
    "devo":  "npm run setup && npm run watch",
    "setup":  "docker compose up -d && npm run csv-agno && npm run seed-agno && npm run types-agno",
    "watch":"docker compose up --watch",
    "tear":"docker compose down && docker system prune --all --volumes --force",
    "restart":"docker compose down && docker compose build --no-cache",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "csv-manual": "tsx ./src/server/sql_db/convertData_manual.ts",
    "csv-module": "tsx ./src/server/sql_db/convertData_module.ts",
    "csv-agno": "tsx ./src/server/sql_db/convertData_agnostic.ts",
    "seed-supabase": "tsx ./src/server/sql_db/seed.ts supabase",
    "seed-local": "tsx ./src/server/sql_db/seed_local.ts",
    "seed-agno": "tsx ./src/server/sql_db/seed_agnostic.ts",
    "types-supabase": "tsx ./src/server/sql_db/generateSchema.ts",
    "types-local": "tsx ./src/server/sql_db/generateSchema_local.ts",
    "types-agno": "tsx ./src/server/sql_db/generateSchema_agnostic.ts",
    "db-setup-agno": "npm run csv-agno && npm run seed-agno && npm run types-agno",
    "db-setup-localLinux": "npm run csv-module && npm run seed-local && npm run types-local"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "node-cache": "^5.1.2",
    "pg": "^8.18.0",
    "pg-copy-streams": "^7.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-select": "^5.10.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@json2csv/plainjs": "^7.0.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/pg-copy-streams": "^1.2.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "csv-parse": "^6.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
